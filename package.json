    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.0",
    "@types/node": "24.10.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3"
